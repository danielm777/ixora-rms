				<patternset><include name="com.ixora.rms.ui.dataviewboard.**.*Board"/></patternset>
				<patternset><include name="com.ixora.rms.ui.dataviewboard.**.*CounterFilter"/></patternset>
				<patternset><include name="com.ixora.common.typedproperties.ui.ExtendedEditor*"/></patternset>
				<patternset><include name="com.ixora.rms.repository.DataView"/></patternset>
				<patternset><include name="com.ixora.rms.ui.dataviewboard.utils.FilterNumericValueDeltaHandler"/></patternset>

