				<patternset><include name="com.ixora.rms.agents.**.*Configuration*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.**.*Agent"/></patternset>
				<patternset><include name="com.ixora.rms.EntityDescriptorTree"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentState"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentId"/></patternset>
				<patternset><include name="com.ixora.rms.EntityId"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentDataBuffer"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentPollBuffer"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentConfiguration"/></patternset>
				<patternset><include name="com.ixora.rms.EntityConfiguration"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentActivationTuple"/></patternset>
				<patternset><include name="com.ixora.rms.agents.AgentConfigurationTuple"/></patternset>

