				<patternset><include name="com.ixora.rms.data.CounterValue*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.windows.WinPerfRootEntity*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.windows.Native*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.**.*Exception*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.**.exception.*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.db2.DB2RootEntity*"/></patternset>
				<patternset><include name="com.ixora.common.net.icmp.ICMP*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.wmi.WMIRootEntity*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.wmi.NativeWMIProperty*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.wmi.NativeWMIRelationship*"/></patternset>

				<!-- expose Websphere PMI stub -->
