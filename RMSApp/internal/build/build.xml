	   <inoutpair in="${destDir}/xraw/AgentWeblogic_ui.jar" out="${destDir}/AgentWeblogic_ui.jar"/>
       <inoutpair in="${destDir}/xraw/RMS.jar" out="${destDir}/RMS.jar"/>
       <inoutpair in="${destDir}/xraw/RMSCommon.jar" out="${destDir}/RMSCommon.jar"/>
		<inoutpair in="${destDir}/xraw/RMSJMX.jar" out="${destDir}/RMSJMX.jar"/>
		<inoutpair in="${destDir}/xraw/AgentSunApp.jar" out="${destDir}/AgentSunApp.jar"/>
		<inoutpair in="${destDir}/xraw/AgentSunApp_ui.jar" out="${destDir}/AgentSunApp_ui.jar"/>
		<inoutpair in="${destDir}/xraw/AgentJBoss.jar" out="${destDir}/AgentJBoss.jar"/>
       	<inoutpair in="${destDir}/xraw/AgentJBoss_ui.jar" out="${destDir}/AgentJBoss_ui.jar"/>
       	<inoutpair in="${destDir}/xraw/AgentJMXJSR160.jar" out="${destDir}/AgentJMXJSR160.jar"/>
       	<inoutpair in="${destDir}/xraw/AgentJMXJSR160_ui.jar" out="${destDir}/AgentJMXJSR160_ui.jar"/>
