				<patternset><include name="com.ixora.jobs.remote.JobManagerImpl_Stub"/></patternset>
				<patternset><include name="com.ixora.jobs.remote.JobManager"/></patternset>
				<patternset><include name="com.ixora.jobs.JobLogEvent"/></patternset>
				<patternset><include name="com.ixora.jobs.JobStateEvent"/></patternset>
				<patternset><include name="com.ixora.jobs.JobDefinition"/></patternset>
				<patternset><include name="com.ixora.jobs.impl.command.ExternalCommandJobData"/></patternset>

