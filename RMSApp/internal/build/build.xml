				<patternset><include name="com.ixora.rms.agents.websphere.*.proxy.PmiClientProxy"/></patternset>
				<patternset><include name="com.ixora.rms.agents.websphere.*.proxy.PmiClientProxyImpl"/></patternset>
				<patternset><include name="com.ixora.rms.agents.websphere.*.proxy.PerfDescriptor"/></patternset>

				<!-- expose Providers for remote access -->
				<patternset><include name="com.ixora.rms.remote.providers.RemoteProviderManager*"/></patternset>
				<patternset><include name="com.ixora.rms.providers.ProviderState"/></patternset>
				<patternset><include name="com.ixora.rms.providers.ProviderId"/></patternset>
				<patternset><include name="com.ixora.rms.providers.ProviderDataBuffer"/></patternset>
				<patternset><include name="com.ixora.rms.providers.ProviderPollBuffer"/></patternset>
				<patternset><include name="com.ixora.rms.providers.ProviderConfiguration"/></patternset>
				<patternset><include name="com.ixora.rms.providers.ProviderCustomConfiguration"/></patternset>
				<patternset><include name="com.ixora.rms.providers.impl.**.*Configuration*"/></patternset>
				<patternset><include name="com.ixora.rms.providers.impl.**.Provider*"/></patternset>
				<patternset><include name="com.ixora.rms.repository.ProviderInstance"/></patternset>
				<patternset><include name="com.ixora.rms.repository.AgentTemplate"/></patternset>
				<patternset><include name="com.ixora.rms.agents.providers.parsers.*.ui.*Panel"/></patternset>
				<patternset><include name="com.ixora.rms.agents.providers.parsers.*.*Parser"/></patternset>
				<patternset><include name="com.ixora.rms.agents.providers.parsers.*.*Definition"/></patternset>

				<!-- expose filters -->
				<patternset><include name="com.ixora.common.ui.filter.FilterDate"/></patternset>
				<patternset><include name="com.ixora.common.ui.filter.FilterString"/></patternset>
				<patternset><include name="com.ixora.common.ui.filter.FilterRegex"/></patternset>
				<patternset><include name="com.ixora.common.ui.filter.FilterNumber"/></patternset>

				<!-- LogFile monitor agent -->
				<patternset><include name="com.ixora.rms.agents.logfile.definitions.StoredLogParserDef"/></patternset>
				<patternset><include name="com.ixora.rms.agents.logfile.LogParserDefinition"/></patternset>
				<patternset><include name="com.ixora.rms.agents.logfile.LogParserScript"/></patternset>

				<!-- URL monitor agent -->
				<patternset><include name="com.ixora.rms.agents.url.RequestParameters"/></patternset>

				<!-- expose extension points -->
				<patternset><include name="com.ixora.rms.providers.impl.java.JavaProviderImplementation"/></patternset>
				<patternset><include name="com.ixora.rms.providers.impl.java.JavaProviderImplementationContext"/></patternset>
				<patternset><include name="com.ixora.rms.providers.utils.sql.*"/></patternset>
				<patternset><include name="com.ixora.rms.providers.utils.sql.exception.*"/></patternset>
				<patternset><include name="com.ixora.rms.agents.impl.logfile.LogRecord"/></patternset>
				<patternset><include name="com.ixora.rms.agents.impl.logfile.LogRecordBatch"/></patternset>
				<patternset><include name="com.ixora.rms.agents.logfile.LogParserScriptContext"/></patternset>
				<patternset><include name="com.ixora.rms.dataengine.reactions.ReactionEnvironment"/></patternset>

				<!-- expose various other stuff -->
				<patternset><include name="com.ixora.common.logging.adapters.**"/></patternset>
				<patternset><include name="com.ixora.common.logging.**"/></patternset>
				<patternset>
					<include name="com.ixora.**.exception*.**"/>
					<exclude name="com.ixora.common.security.**.exception*.**"/>
				</patternset>
			</class>
			<!-- expose main entry points -->
			<class name="com.ixora.rms.ui.starter.RMSStarter" methods="public" fields="none"/>
			<class name="com.ixora.rms.remote.starter.HostManagerStarter" methods="public" fields="none"/>
			<class name="com.ixora.rms.remote.starter.HostManagerStopper" methods="public" fields="none"/>
			<class name="com.ixora.rms.batch.starter.BatchStopper" methods="public" fields="none"/>
			<class name="com.ixora.rms.batch.starter.BatchStarter" methods="public" fields="none"/>
			<class name="com.ixora.rms.ui.RMSFrame" methods="public" fields="none"/>
			<class name="com.ixora.rms.remote.HostManagerMain" methods="public" fields="none"/>
			<class name="com.ixora.rms.batch.BatchMain" methods="public" fields="none"/>

			  <field name="serialVersionUID">
			   <patternset>
			    <include name="**.*"/>
			   </patternset>
			  </field>

			<!-- don't obfuscate at all the following -->
			<class methods="private" fields="private">
				<patternset><include name="org.jfree.**/*"/></patternset>
				<patternset><include name="thor.net.**/*"/></patternset>
				<patternset><include name="ch.ethz.**/*"/></patternset>
				<patternset><include name="com.ixora.rms.ui.dataviewboard.**.definitions.*"/></patternset>
				<patternset><include name="**.percederberg.**"/></patternset>
				<patternset>
					<include name="**.snmp.MIBNode"/>
					<include name="**.snmp.MIBTree"/>
				</patternset>

				<!-- comment the line below for obfuscation -->
				<patternset><include name="**/*"/></patternset>
			</class>
		</expose>
    </obfuscate>
</target>

