       <inoutpair in="${destDir}/xraw/AgentServiceAvailability.jar" out="${destDir}/AgentServiceAvailability.jar"/>
       <inoutpair in="${destDir}/xraw/AgentSQL.jar" out="${destDir}/AgentSQL.jar"/>
       <inoutpair in="${destDir}/xraw/AgentWebsphere_ui.jar" out="${destDir}/AgentWebsphere_ui.jar"/>
       <inoutpair in="${destDir}/xraw/AgentWebsphere.jar" out="${destDir}/AgentWebsphere.jar"/>
       <inoutpair in="${destDir}/xraw/AgentWebsphereProxy.jar" out="${destDir}/AgentWebsphereProxy.jar"/>
       <inoutpair in="${destDir}/xraw/AgentWindows.jar" out="${destDir}/AgentWindows.jar"/>
