<?xml version="1.0" encoding="UTF-8"?>
<rms>
  <session name="logfile">
    <hosts>
      <host name="localhost">
        <agents>
          <agent>
            <instanceData>
              <activation>
                <location>0</location>
                <host>localhost</host>
                <agentInstallationId>agents.logfile</agentInstallationId>
                <config>
                  <sampling>-1</sampling>
                  <config class="com.ixora.rms.agents.logfile.Configuration">
                    <log_file_path type="0" value="C:\jboss-4.0.3RC1\server\default\log\server.log"/>
                    <file_encoding type="0" value="UTF-8"/>
                    <available_log_parsers set="Sun Application Server 8.x,Websphere 6.x,Websphere 5.x,Weblogic 8.x,Weblogic 9.x,JBoss 4.x" type="0" value="JBoss 4.x"/>
                    <current_log_parser notvisible="true" type="9" value=""/>
                  </config>
                </config>
              </activation>
              <agentDescriptor>
                <agentId>agents.logfile</agentId>
                <config>
                  <sampling>-1</sampling>
                  <config class="com.ixora.rms.agents.logfile.Configuration">
                    <log_file_path type="0" value="C:\jboss-4.0.3RC1\server\default\log\server.log"/>
                    <file_encoding type="0" value="UTF-8"/>
                    <available_log_parsers set="Sun Application Server 8.x,Websphere 6.x,Websphere 5.x,Weblogic 8.x,Weblogic 9.x,JBoss 4.x" type="0" value="JBoss 4.x"/>
                    <current_log_parser notvisible="true" type="9" value=""/>
                  </config>
                </config>
              </agentDescriptor>
            </instanceData>
            <entities/>
          </agent>
        </agents>
      </host>
    </hosts>
    <queries>
      <query>
        <query id="localhost/agents.logfile/{Log records}">
          <resource id="log_records" iname="$counter" rid="-/-/root/(.*)/[log_records]" type="object"/>
          <reaction params="log_records" severity="HIGH">
            <arm><![CDATA[String match="Exception";java.util.List records = ((com.ixora.rms.agents.impl.logfile.LogRecordBatch)log_records).getLogRecords();return false;]]></arm>
            <disarm><![CDATA[return env.getSecondsSinceLastArmed() > 20;]]></disarm>
            <fire><![CDATA[return env.getSecondsSinceArmed() >= 0;]]></fire>
            <delivery><![CDATA[env.email();]]></delivery>
            <message><![CDATA[return "Application exception found in the log file";]]></message>
          </reaction>
        </query>
      </query>
    </queries>
    <dashboards/>
    <screens>
      <screen>
        <name>Untitled</name>
        <selected>true</selected>
        <viewboards>
          <viewboard board="com.ixora.rms.ui.dataviewboard.logs.LogBoard" h="550.0" title="Dashboard 1" w="600.0" x="40.0" y="40.0">
            <controls>
              <control class="com.ixora.rms.ui.dataviewboard.logs.LogControlDescriptor">
                <view class="com.ixora.rms.ui.dataviewboard.logs.LogCounterDataView">
                  <name>Log records</name>
                  <description>Log records</description>
                  <query>
                    <resource id="log_records" iname="$counter" rid="-/-/root/(.*)/[log_records]" type="object"/>
                    <reaction params="log_records" severity="HIGH">
                      <arm><![CDATA[String match="Exception";java.util.List records = ((com.ixora.rms.agents.impl.logfile.LogRecordBatch)log_records).getLogRecords();return false;]]></arm>
                      <disarm><![CDATA[return env.getSecondsSinceLastArmed() > 20;]]></disarm>
                      <fire><![CDATA[return env.getSecondsSinceArmed() >= 0;]]></fire>
                      <delivery><![CDATA[env.email();]]></delivery>
                      <message><![CDATA[return "Application exception found in the log file";]]></message>
                    </reaction>
                  </query>
                  <agentVersions/>
                  <author>system</author>
                </view>
                <context id="localhost/agents.logfile"/>
                <config sortDirectionDesc="false" sortedColumnIdx="-1"/>
              </control>
            </controls>
          </viewboard>
        </viewboards>
      </screen>
    </screens>
  </session>
</rms>
