        <resource id="percentused" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]" name="$counter" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[connectionPoolModule.percentUsed]"/>
      </query>
      <agentVersions>
        <agentVersion>5.1.*</agentVersion>
        <agentVersion>5.0.*</agentVersion>
