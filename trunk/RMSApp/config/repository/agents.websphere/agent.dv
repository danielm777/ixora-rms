    <view class="com.ixora.rms.ui.dataviewboard.charts.definitions.ChartDef">
      <name>Thread pools utilisation</name>
      <description>Thread pools, active threads(bar chart)</description>
      <query>
        <resource id="pool" iname="$entity[1]/$entity[2]/$entity[4]" name="Thread pool" rid="-/-/root/(.*)/(.*)/threadPoolModule/(.*)/"/>
        <resource id="active" iname="$entity[1]/$entity[2]/$entity[4]/$counter" name="$counter" rid="-/-/root/(.*)/(.*)/threadPoolModule/(.*)/[threadPoolModule.activeThreads]"/>
        <resource id="size" iname="$entity[1]/$entity[2]/$entity[4]/$counter" name="$counter" rid="-/-/root/(.*)/(.*)/threadPoolModule/(.*)/[threadPoolModule.poolSize]"/>
        <reaction params="pool,active,size" severity="HIGH">
          <arm><![CDATA[return size > 0 && active == size;]]></arm>
          <disarm><![CDATA[return env.getSecondsSinceLastArmed() > 20;]]></disarm>
          <fire><![CDATA[return env.getSecondsSinceArmed() > 60;]]></fire>
          <delivery><![CDATA[env.email();]]></delivery>
          <message><![CDATA[return "Thread pool " + pool + " has been maximized for the last minute.";]]></message>
          <advise><![CDATA[return "If there are no other bottlenecks try and increase the size of the threadpool until the CPU usage is maximized.";]]></advise>
        </reaction>
      </query>
      <agentVersions>
        <agentVersion>5.1.*</agentVersion>
        <agentVersion>5.0.*</agentVersion>
      </agentVersions>
      <author>system</author>
      <renderer type="Bar2D">
        <domain id="pool"/>
        <range id="active"/>
        <range id="size"/>
      </renderer>
    </view>
