        <range id="closes"/>
      </renderer>
    </view>
    <view class="com.ixora.rms.ui.dataviewboard.charts.definitions.ChartDef">
      <name>JDBC SQL statement discards</name>
      <description>JDBC statement discards(bar chart); for a well tuned system there should be no discards and therefore no data shown in this view</description>
      <query>
        <resource id="datasource" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]" name="Datasource" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)"/>
        <resource id="discards" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]/$counter" name="$counter" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[connectionPoolModule.prepStmtCacheDiscards]"/>
        <reaction params="datasource,discards" severity="HIGH">
          <arm><![CDATA[return discards > 30;]]></arm>
          <disarm><![CDATA[return env.getSecondsSinceLastArmed() > 20;]]></disarm>
          <fire><![CDATA[return env.getSecondsSinceArmed() > 60;]]></fire>
          <delivery><![CDATA[env.email();]]></delivery>
          <message><![CDATA[return "The prepared statement discards for " + datasource + " is greater than 30";]]></message>
          <advise><![CDATA[return "Increase the size of the prepared statement cache until there are no discards.";]]></advise>
        </reaction>
      </query>
      <agentVersions>
        <agentVersion>5.1.*</agentVersion>
        <agentVersion>5.0.*</agentVersion>
      </agentVersions>
      <author>system</author>
      <renderer type="Bar2D">
        <domain id="datasource"/>
        <range id="discards"/>
      </renderer>
    </view>
