    <view class="com.ixora.rms.ui.dataviewboard.charts.definitions.ChartDef">
      <name>JDBC connection pools utilisation</name>
      <description>JDBC connection pools, active connections(bar chart)</description>
      <query>
        <resource id="pool" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)"/>
        <resource id="free" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]/$counter" name="$counter" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[connectionPoolModule.freePoolSize]"/>
        <resource id="size" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]/$counter" name="$counter" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[connectionPoolModule.poolSize]"/>
        <function id="active" name="Active Connections" op="script">
          <param id="free"/>
          <param id="size"/>
          <code>return size-free;</code>
        </function>
      </query>
      <agentVersions>
        <agentVersion>5.1.*</agentVersion>
        <agentVersion>5.0.*</agentVersion>
      </agentVersions>
      <author>system</author>
      <renderer type="Bar2D">
        <domain id="pool"/>
        <range id="active"/>
        <range id="size"/>
      </renderer>
    </view>
    <view class="com.ixora.rms.ui.dataviewboard.charts.definitions.ChartDef">
      <name>JDBC connections closed</name>
      <description>JDBC connections closed(bar chart); this value should be very small and during operational workload it should be zero</description>
      <query>
        <resource id="datasource" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]" name="Connection pool" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)"/>
        <resource id="closes" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]" name="$counter" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[connectionPoolModule.numDestroys]"/>
      </query>
      <agentVersions>
