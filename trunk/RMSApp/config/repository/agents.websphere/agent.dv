    <view class="com.ixora.rms.ui.dataviewboard.charts.definitions.ChartDef">
      <name>Use time for JDBC connections</name>
      <description>Use time for JDBC connections for all servers(time series)</description>
      <query>
        <resource id="time" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[#time#]"/>
        <resource id="usetime" iname="$entity[1]/$entity[2]/$entity[4]/$entity[5]" name="$counter" rid="-/-/root/(.*)/(.*)/connectionPoolModule/(.*)/(.*)/[connectionPoolModule.avgUseTime]"/>
      </query>
      <agentVersions>
        <agentVersion>5.1.*</agentVersion>
        <agentVersion>5.0.*</agentVersion>
      </agentVersions>
      <author>system</author>
      <renderer type="XYLine">
        <domain id="time"/>
        <range id="usetime"/>
      </renderer>
    </view>
    <view class="com.ixora.rms.ui.dataviewboard.tables.definitions.TableDef">
