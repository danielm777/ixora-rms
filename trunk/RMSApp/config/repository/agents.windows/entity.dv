    <entity id="root/Network Interface/XPC 802.11b_g Wireless Kit - Packet Scheduler Miniport">
      <views>
        <view class="com.ixora.rms.ui.dataviewboard.charts.definitions.ChartDef">
          <name>Megabytes received/sent per second for all interfaces</name>
          <description>Shows received/sent megabytes per second as a bar chart</description>
          <query>
            <resource code="return id7/Math.pow(2, 20);" id="id7" iname="$host/$agent/$entity[1]/$entity[2]/MB sent/sec" name="MB sent/sec" rid="-/-/root/Network Interface/(.*)/[Bytes Sent/sec]"/>
            <resource code="return id9/Math.pow(2, 20);" id="id9" iname="$host/$agent/$entity[1]/$entity[2]/MB recv/sec" name="MB recv/sec" rid="-/-/root/Network Interface/(.*)/[Bytes Received/sec]"/>
            <resource id="id0" iname="$host/$agent/$entity[1]/$entity[2]" name="Network Interface" rid="-/-/root/Network Interface/(.*)"/>
          </query>
          <agentVersions>
            <agentVersion>Win2003</agentVersion>
          </agentVersions>
          <author>system</author>
          <renderer type="StackedBar2D">
            <domain id="id0"/>
            <range id="id7"/>
            <range id="id9"/>
          </renderer>
        </view>
      </views>
    </entity>
