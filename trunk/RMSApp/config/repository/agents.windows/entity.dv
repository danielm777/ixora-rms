            <resource id="writeop" iname="$host/$entity[1]/$counter" name="$counter" rid="-/-/-/[File Write Operations/sec]"/>
            <resource id="ctlop" iname="$host/$entity[1]/$counter" name="$counter" rid="-/-/-/[File Control Operations/sec]"/>
          </query>
          <agentVersions>
            <agentVersion>Win2003</agentVersion>
            <agentVersion>WinXP</agentVersion>
            <agentVersion>Win2000</agentVersion>
          </agentVersions>
          <author>system</author>
          <renderer type="StackedXYArea">
            <domain id="time"/>
            <range id="readop"/>
            <range id="writeop"/>
            <range id="ctlop"/>
          </renderer>
        </view>
      </views>
    </entity>
