          <query>
            <resource id="processor" iname="$entity[2]" name="Processor" rid="-/-/root/Processor/(.*)"/>
            <resource id="proctime" iname="$entity[2]/% Processor Time" max="100.0" name="% Processor Time" rid="-/-/root/Processor/(.*)/[% Processor Time]"/>
            <resource id="usertime" iname="$entity[2]/% User Time" max="100.0" name="% User Time" rid="-/-/root/Processor/(.*)/[% User Time]"/>
            <resource id="privtime" iname="$entity[2]/% Privileged Time" max="100.0" name="% Privileged Time" rid="-/-/root/Processor/(.*)/[% Privileged Time]"/>
            <resource id="dpctime" iname="$entity[2]/% DPC Time" max="100.0" name="% DPC Time" rid="-/-/root/Processor/(.*)/[% DPC Time]"/>
            <resource id="inttime" iname="$entity[2]/% Interrupt Time" max="100.0" name="% Interrupt Time" rid="-/-/root/Processor/(.*)/[% Interrupt Time]"/>
            <reaction params="usertime" severity="CRITICAL">
              <arm><![CDATA[return usertime > 0.95;]]></arm>
              <disarm><![CDATA[return env.getSecondsSinceLastArmed() > 60;]]></disarm>
              <fire><![CDATA[return env.getSecondsSinceArmed() > 60;]]></fire>
              <delivery><![CDATA[env.email(); env.job("libJob");]]></delivery>
              <message><![CDATA[return "The CPU usage for " + env.getHost(usertime) + " has been greater than 95% for one minute";]]></message>
              <advise><![CDATA[return "The CPU usage for " + env.getHost(usertime) + " has been greater than 95% for one minute";]]></advise>
            </reaction>
          </query>
          <agentVersions>
            <agentVersion>Win2003</agentVersion>
          </agentVersions>
          <author>system</author>
          <category id="processor"/>
        </view>
        <view class="com.ixora.rms.ui.dataviewboard.tables.definitions.TableDef">
          <name>Usage per processor</name>
          <description>Shows usage detailed per processor</description>
          <query>
            <resource id="processor" iname="$entity[2]" name="Processor" rid="-/-/root/Processor/(.*)"/>
            <resource id="proctime" iname="$host/$entity[1]/$entity[2]/$counter" max="100.0" name="$counter" rid="-/-/root/Processor/(.*)/[% Processor Time]"/>
            <resource id="usertime" iname="$host/$entity[1]/$entity[2]/$counter" max="100.0" name="$counter" rid="-/-/root/Processor/(.*)/[% User Time]"/>
            <resource id="privtime" iname="$host/$entity[1]/$entity[2]/$counter" max="100.0" name="$counter" rid="-/-/root/Processor/(.*)/[% Privileged Time]"/>
