            <resource id="inttime" iname="$host/$entity[1]/$entity[2]/$counter" max="100.0" name="$counter" rid="-/-/root/Processor/(.*)/[% Interrupt Time]"/>
            <reaction params="usertime,privtime" severity="HIGH">
              <arm><![CDATA[return usertime + privtime > 90;]]></arm>
              <disarm><![CDATA[return env.getSecondsSinceLastArmed() > 20;]]></disarm>
              <fire><![CDATA[return env.getSecondsSinceArmed() > 60;]]></fire>
              <delivery><![CDATA[env.email();]]></delivery>
              <message><![CDATA[return "The usage for CPU number " + env.getEntityPart(usertime, 2) + " on host " + env.getHost(usertime) + " has been greater than 90% for the last minute";]]></message>
