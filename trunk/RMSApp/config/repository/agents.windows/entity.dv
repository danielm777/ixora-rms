    <entity id="root/.NET CLR Memory">
      <views>
        <view class="com.ixora.rms.ui.dataviewboard.tables.definitions.TableDef">
          <name>Statistics per application</name>
          <description>Garbage collection statistics detailed per application</description>
          <query>
            <resource id="appl" iname="$entity[2]" name="Application" rid="-/-/root/.NET CLR Memory/(.*)"/>
            <resource id="gctime" iname="$entity[2]/GC Time" name="GC Time" rid="-/-/root/.NET CLR Memory/(.*)/[% Time in GC]"/>
            <resource code="return totalreserved/1048576;" id="totalreserved" iname="$entity[2]/Reserved MB" name="Reserved MB" rid="-/-/root/.NET CLR Memory/(.*)/[# Total reserved Bytes]"/>
            <resource code="return bytesallheaps/1048576;" id="bytesallheaps" iname="$entity[2]/All heaps MB" name="All heaps MB" rid="-/-/root/.NET CLR Memory/(.*)/[# Bytes in all Heaps]"/>
            <resource id="bytespersec" iname="$entity[2]/$counter" name="Allocated bytes/sec" rid="-/-/root/.NET CLR Memory/(.*)/[Allocated Bytes/sec]"/>
            <resource code="return comittedbytes/1048576;" id="comittedbytes" iname="$entity[2]/Comitted MB" name="Comitted MB" rid="-/-/root/.NET CLR Memory/(.*)/[# Total committed Bytes]"/>
            <resource code="return loheapsize/1048576;" id="loheapsize" iname="$entity[2]/Large obj heap MB" name="Large obj heap MB" rid="-/-/root/.NET CLR Memory/(.*)/[Large Object Heap size]"/>
          </query>
          <agentVersions>
            <agentVersion>Win2003</agentVersion>
            <agentVersion>WinXP</agentVersion>
            <agentVersion>Win2000</agentVersion>
          </agentVersions>
          <author>system</author>
          <category id="appl"/>
        </view>
        <view class="com.ixora.rms.ui.dataviewboard.properties.definitions.PropertiesDef">
          <name>Generic statistics</name>
          <description>Generic statistics related to garbage collection</description>
          <query>
            <resource id="gctime" iname="$counter" rid="-/-/root/.NET CLR Memory/[% Time in GC]"/>
            <resource code="return totalreserved/1048576;" id="totalreserved" iname="Total reserved MB" rid="-/-/root/.NET CLR Memory/[# Total reserved Bytes]"/>
            <resource code="return bytesallheaps/1048576;" id="bytesallheaps" iname="All heaps MB" rid="-/-/root/.NET CLR Memory/[# Bytes in all Heaps]"/>
            <resource id="bytespersec" iname="$counter" rid="-/-/root/.NET CLR Memory/[Allocated Bytes/sec]"/>
            <resource code="return comittedbytes/1048576;" id="comittedbytes" iname="Total committed MB" rid="-/-/root/.NET CLR Memory/[# Total committed Bytes]"/>
            <resource code="return loheapsize/1048576;" id="loheapsize" iname="Large obj heap MB" rid="-/-/root/.NET CLR Memory/[Large Object Heap size]"/>
          </query>
          <agentVersions>
            <agentVersion>Win2003</agentVersion>
            <agentVersion>WinXP</agentVersion>
            <agentVersion>Win2000</agentVersion>
          </agentVersions>
          <author>system</author>
        </view>
      </views>
    </entity>
