<rms>
	<!-- Style 'percentage', useful for values between 0 and 100 --> 
	<style id="percentage" continuous="true" max="100" format="#,###,###,###,###,###.####'%'"/>
	
	<!-- Extends 'percentage' and adds name, description -->
	<style id="CPU" name="CPU usage" description="CPU usage" style="percentage" />
</rms>

