<xml>
<!-- Stilul 'percentage' util pentru orice valuri continue intre 0 si 100 --> 
<style id="percentage" continuous="true"
	format="#,###,###,###,###,###.#### '%'"/>

<!-- Extinde stilul 'percentage' si adauga name, description si stacked-->
<style id="CPU" name="CPU" description="the CPU dimension" 
	stacked="true" style="percentage"/>
</xml>

