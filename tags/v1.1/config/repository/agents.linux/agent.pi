    <providerInstance class="com.ixora.rms.repository.ProviderInstance">
      <providerName>providers.process.name</providerName>
      <instanceName>System data</instanceName>
      <description>General system data provided by vmstat</description>
      <author>system</author>
      <remote>true</remote>
      <optional>true</optional>
      <selectedByDefault>true</selectedByDefault>
      <inheritsLocationFromAgent>true</inheritsLocationFromAgent>
      <config>
        <config class="com.ixora.rms.providers.impl.process.Configuration">
          <providers.process.command type="0" value="vmstat {tick}"/>
          <providers.process.is_volatile notrequired="true" type="3" value="false"/>
          <providers.process.end_of_buffer_marker notrequired="true" type="0" value=""/>
          <providers.process.execution_mode notrequired="true" set="{agent.ExecutionMode},Normal,Telnet,SSH2" type="0" value="{agent.ExecutionMode}"/>
          <providers.process.username notrequired="true" type="0" value="{agent.Username}"/>
          <providers.process.password notrequired="true" type="0" value="{agent.Password}"/>
          <providers.process.username_prompt notrequired="true" type="0" value="{agent.UsernamePrompt}"/>
          <providers.process.password_prompt notrequired="true" type="0" value="{agent.PasswordPrompt}"/>
          <providers.process.shell_prompt notrequired="true" type="0" value="{agent.ShellPrompt}"/>
          <providers.process.port notrequired="true" type="0" value="{agent.Port}"/>
        </config>
        <usePrivateCollector>false</usePrivateCollector>
      </config>
      <parserInstance>
        <parser>providers.parsers.table.name</parser>
        <rules class="com.ixora.rms.agents.providers.parsers.table.TableRulesDefinition">
          <columnSeparator> 	</columnSeparator>
          <columns>
            <column cid="Processes waiting for run time" eid="root/System" idx="0"/>
            <column cid="Processes in uninterruptable sleep" eid="root/System" idx="1"/>
            <column cid="Processes swapped out but runnable" eid="root/System" idx="2"/>
            <column cid="Virtual memory used (kB)" eid="root/Memory" idx="3"/>
            <column cid="Free memory (kB)" eid="root/Memory" idx="4"/>
            <column cid="Buffer memory (kB)" eid="root/Memory" idx="5"/>
            <column cid="Cache memory (kB)" eid="root/Memory" idx="6"/>
            <column cid="Memory swapped in (kB/s)" eid="root/Swap" idx="7"/>
            <column cid="Memory swapped out (kB/s)" eid="root/Swap" idx="8"/>
            <column cid="Blocks sent per second" eid="root/IO" idx="9"/>
            <column cid="Blocks received per second" eid="root/IO" idx="10"/>
            <column cid="Interrupts per second" eid="root/System" idx="11"/>
            <column cid="Context switches per second" eid="root/System" idx="12"/>
            <column cid="User time" eid="root/CPU" idx="13"/>
            <column cid="System time" eid="root/CPU" idx="14"/>
            <column cid="Idle time" eid="root/CPU" idx="15"/>
          </columns>
          <ignoreLines>buff|procs|vmstat</ignoreLines>
          <accumulateVolatileEntities>false</accumulateVolatileEntities>
        </rules>
      </parserInstance>
      <descriptors>
        <entitydescriptor id="root/CPU">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Idle time">
            <alternatename/>
            <description>Percentage of CPU time where CPU was idle</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="User time">
            <alternatename/>
            <description>Percentage of CPU time where CPU was executing user code</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="System time">
            <alternatename/>
            <description>Percentage of CPU time where CPU was executing system code</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
        <entitydescriptor id="root/Swap">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Memory swapped in (kB/s)">
            <alternatename/>
            <description>The amount of memory swapped in from disk (kB/s)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Memory swapped out (kB/s)">
            <alternatename/>
            <description>The amount of memory swapped to disk (kB/s)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
        <entitydescriptor id="root/System">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Context switches per second">
            <alternatename/>
            <description>The number of context switches per second</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Processes in uninterruptable sleep">
            <alternatename/>
            <description>The number of processes in uninterruptable sleep</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Interrupts per second">
            <alternatename/>
            <description>The number of interrupts per second, including the clock</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Processes swapped out but runnable">
            <alternatename/>
            <description>The number of processes swapped out but otherwise runnable.  This           field is calculated, but Linux never desperation swaps</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Processes waiting for run time">
            <alternatename/>
            <description>The number of processes waiting for run time</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
        <entitydescriptor id="root/Memory">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Cache memory (kB)">
            <alternatename/>
            <description>The amount of memory used for cache (kB)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Free memory (kB)">
            <alternatename/>
            <description>The amount of idle memory (kB)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Buffer memory (kB)">
            <alternatename/>
            <description>The amount of memory used as buffers (kB)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Virtual memory used (kB)">
            <alternatename/>
            <description>The amount of virtual memory used (kB)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
        <entitydescriptor id="root/IO">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Blocks sent per second">
            <alternatename/>
            <description>Blocks sent to a block device (blocks/s)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Blocks received per second">
            <alternatename/>
            <description>Blocks received from a block device (blocks/s)</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
      </descriptors>
      <agentVersions>
        <agentVersion>RedHat 9</agentVersion>
      </agentVersions>
    </providerInstance>
    <providerInstance class="com.ixora.rms.repository.ProviderInstance">
      <providerName>providers.process.name</providerName>
      <instanceName>File system data</instanceName>
      <description>File system data provided by df</description>
      <author>system</author>
      <remote>false</remote>
      <optional>true</optional>
      <selectedByDefault>true</selectedByDefault>
      <inheritsLocationFromAgent>true</inheritsLocationFromAgent>
      <config>
        <config class="com.ixora.rms.providers.impl.process.Configuration">
          <providers.process.command type="0" value="df --block-size=1M -P"/>
          <providers.process.is_volatile notrequired="true" type="3" value="true"/>
          <providers.process.end_of_buffer_marker notrequired="true" type="0" value=""/>
          <providers.process.execution_mode notrequired="true" set="{agent.ExecutionMode},Normal,Telnet,SSH2" type="0" value="{agent.ExecutionMode}"/>
          <providers.process.username notrequired="true" type="0" value="{agent.Username}"/>
          <providers.process.password notrequired="true" type="0" value="{agent.Password}"/>
          <providers.process.username_prompt notrequired="true" type="0" value="{agent.UsernamePrompt}"/>
          <providers.process.password_prompt notrequired="true" type="0" value="{agent.PasswordPrompt}"/>
          <providers.process.shell_prompt notrequired="true" type="0" value="{agent.ShellPrompt}"/>
          <providers.process.port notrequired="true" type="0" value="{agent.Port}"/>
        </config>
        <usePrivateCollector>false</usePrivateCollector>
      </config>
      <parserInstance>
        <parser>providers.parsers.table.name</parser>
        <rules class="com.ixora.rms.agents.providers.parsers.table.TableRulesDefinition">
          <columnSeparator> 	</columnSeparator>
          <columns>
            <column eid="root/File system/{0(5)}" idx="0"/>
            <column cid="Total 1MB blocks" eid="root/File system/{0(5)}" idx="1"/>
            <column cid="Blocks used" eid="root/File system/{0(5)}" idx="2"/>
            <column cid="Blocks available" eid="root/File system/{0(5)}" idx="3"/>
            <column eid="root/File system/{0(5)}" idx="4"/>
            <column eid="root/File system/{0(5)}" idx="5"/>
          </columns>
          <ignoreLines>1M-blocks*|block-size*</ignoreLines>
          <accumulateVolatileEntities>true</accumulateVolatileEntities>
        </rules>
      </parserInstance>
      <descriptors>
        <entitydescriptor id="root/File system/{0(5)}">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Blocks used">
            <alternatename/>
            <description>Number of 1MB blocks used</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Total 1MB blocks">
            <alternatename/>
            <description>Total number of 1MB blocks</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Blocks available">
            <alternatename/>
            <description>Number of 1MB blocks available</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
      </descriptors>
      <agentVersions>
        <agentVersion>RedHat AS 3</agentVersion>
        <agentVersion>RedHat 9</agentVersion>
      </agentVersions>
    </providerInstance>
    <providerInstance class="com.ixora.rms.repository.ProviderInstance">
      <providerName>providers.process.name</providerName>
      <instanceName>Network interfaces</instanceName>
      <description>Network interface data provided by netstat</description>
      <author>system</author>
      <remote>false</remote>
      <optional>true</optional>
      <selectedByDefault>true</selectedByDefault>
      <inheritsLocationFromAgent>true</inheritsLocationFromAgent>
      <config>
        <config class="com.ixora.rms.providers.impl.process.Configuration">
          <providers.process.command type="0" value="netstat -i {tick}"/>
          <providers.process.is_volatile notrequired="true" type="3" value="false"/>
          <providers.process.end_of_buffer_marker notrequired="true" type="0" value=""/>
          <providers.process.execution_mode notrequired="true" set="{agent.ExecutionMode},Normal,Telnet,SSH2" type="0" value="{agent.ExecutionMode}"/>
          <providers.process.username notrequired="true" type="0" value="{agent.Username}"/>
          <providers.process.password notrequired="true" type="0" value="{agent.Password}"/>
          <providers.process.username_prompt notrequired="true" type="0" value="{agent.UsernamePrompt}"/>
          <providers.process.password_prompt notrequired="true" type="0" value="{agent.PasswordPrompt}"/>
          <providers.process.shell_prompt notrequired="true" type="0" value="{agent.ShellPrompt}"/>
          <providers.process.port notrequired="true" type="0" value="{agent.Port}"/>
        </config>
        <usePrivateCollector>false</usePrivateCollector>
      </config>
      <parserInstance>
        <parser>providers.parsers.table.name</parser>
        <rules class="com.ixora.rms.agents.providers.parsers.table.TableRulesDefinition">
          <columnSeparator> 	</columnSeparator>
          <columns>
            <column eid="root/Network Interfaces/{0}" idx="0"/>
            <column cid="MTU" eid="root/Network Interfaces/{0}" idx="1"/>
            <column cid="Metric" eid="root/Network Interfaces/{0}" idx="2"/>
            <column cid="Received ok" eid="root/Network Interfaces/{0}" idx="3"/>
            <column cid="Received error" eid="root/Network Interfaces/{0}" idx="4"/>
            <column cid="Received dropped" eid="root/Network Interfaces/{0}" idx="5"/>
            <column cid="Received overruns" eid="root/Network Interfaces/{0}" idx="6"/>
            <column cid="Sent ok" eid="root/Network Interfaces/{0}" idx="7"/>
            <column cid="Sent error" eid="root/Network Interfaces/{0}" idx="8"/>
            <column cid="Sent dropped" eid="root/Network Interfaces/{0}" idx="9"/>
            <column cid="Sent overruns" eid="root/Network Interfaces/{0}" idx="10"/>
            <column cid="Flag" eid="root/Network Interfaces/{0}" idx="11"/>
          </columns>
          <ignoreLines>Iface*|Kernel*|netstat*</ignoreLines>
          <accumulateVolatileEntities>true</accumulateVolatileEntities>
        </rules>
      </parserInstance>
      <descriptors>
        <entitydescriptor id="root/Network Interfaces/{0}">
          <alternatename/>
          <description/>
          <haschildren>false</haschildren>
          <indsampling>false</indsampling>
          <counterdescriptor id="Flag">
            <alternatename/>
            <description>The flags that have been set for this interface. The value of this counter can be a combination of the following: A - the interface receives packets for multicast addresses. B - the interface receives broadcast packets. D - the interface-debugging feature is now activated. L - this is the loopback interface.M - the interface is in promiscuous mode. N - the interface doesn't process trailers on packets. O - the Address Resolution Protocol is turned off on this interface.P - this interface is being used as a point-to-point connection. R - the interface is running. U - the interface has been activated</description>
            <discreet>false</discreet>
            <type>2</type>
          </counterdescriptor>
          <counterdescriptor id="Sent overruns">
            <alternatename/>
            <description>The number of packets dropped due to overrun errors</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Sent error">
            <alternatename/>
            <description>The number of packets transmitted with errors</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Sent ok">
            <alternatename/>
            <description>The number of packets transmitted with no errors</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="MTU">
            <alternatename/>
            <description>The largest number of bytes that can be sent in one transmission by this interface</description>
            <discreet>false</discreet>
            <type>1</type>
          </counterdescriptor>
          <counterdescriptor id="Received dropped">
            <alternatename/>
            <description>The number of packets dropped</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Metric">
            <alternatename/>
            <description>The metric value for this interface</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Received overruns">
            <alternatename/>
            <description>The number of packet overruns</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Received error">
            <alternatename/>
            <description>The number of packets received with errors</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Received ok">
            <alternatename/>
            <description>The number of packets received with no errors</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
          <counterdescriptor id="Sent dropped">
            <alternatename/>
            <description>The number of packets dropped during transmission</description>
            <discreet>false</discreet>
            <type>0</type>
          </counterdescriptor>
        </entitydescriptor>
      </descriptors>
      <agentVersions>
        <agentVersion>RedHat AS 3</agentVersion>
        <agentVersion>RedHat 9</agentVersion>
      </agentVersions>
    </providerInstance>
