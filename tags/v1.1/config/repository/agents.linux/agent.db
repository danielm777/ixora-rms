<?xml version="1.0" encoding="UTF-8"?>
<rms>
  <dashboards>
    <dashboard>
      <name>System overview</name>
      <description>An overview of a Linux system's operational state</description>
      <views>
        <view id="-/-/root/CPU/:CPU usage:"/>
        <view id="-/-/root/Disks/:Disk utilisation:"/>
        <view id="-/-/root/Disks/:Disk queue length:"/>
        <view id="-/-/root/Disks/:Per disk read/write requests per second:"/>
        <view id="-/-/root/File system/:File systems:"/>
        <view id="-/-/root/Network Interfaces/:Network interfaces overview:"/>
        <view id="-/-/root/IO/:I/O activity:"/>
        <view id="-/-/root/Swap/:Swap activity:"/>
      </views>
      <author>system</author>
      <counters>
        <counter id="-/-/root/System/[Processes waiting for run time]"/>
        <counter id="-/-/root/System/[Context switches per second]"/>
        <counter id="-/-/root/System/[Processes swapped out but runnable]"/>
        <counter id="-/-/root/Memory/[Cache memory (kB)]"/>
        <counter id="-/-/root/Memory/[Buffer memory (kB)]"/>
        <counter id="-/-/root/Memory/[Virtual memory used (kB)]"/>
      </counters>
      <agentVersions>
        <agentVersion>RedHat AS 3</agentVersion>
        <agentVersion>RedHat 9</agentVersion>
      </agentVersions>
    </dashboard>
  </dashboards>
</rms>
